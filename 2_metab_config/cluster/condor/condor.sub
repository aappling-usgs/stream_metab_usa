batch_name = powmetab
executable = run_job.sh
arguments = run_job.R $(Process)
transfer_input_files = run_job.R, run_model_job.R, run_model.R, config.tsv, files_metab.tsv, .Renviron, bundle.zip, unzip, stream_metab.yaml
universe = vanilla
output = out/$(Process).out
error = err/$(Process).err
log = log/$(Process).log
stream_output = True
stream_error = True
requirements = (TARGET.Arch == "X86_64") && (TARGET.OpSys == "LINUX")
request_cpus = 4
should_transfer_files = YES
#+WantFlocking = true
when_to_transfer_output = ON_EXIT
notification = never
transfer_output_files = job.zip
transfer_output_remaps = "job.zip = results/job_$(Process).zip"
queue 2
